<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
    <metadata>
        <id>GrammarEngine.Linux</id>
        <version>$version$</version>
        <title>Russian Grammatical Dictionary and Thesaurus (Linux native binaries)</title>
        <authors>Elijah Koziev</authors>
        <owners>Elijah Koziev</owners>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Unoficcial build of the latest Russian Grammatical Dictionary and Thesaurus by Elijah Koziev (http://www.solarix.ru).</description>
        <summary />
        <copyright>Elijah Koziev</copyright>
        <contentFiles>
            <files include="**/Resources/*.*" buildAction="EmbeddedResource" />
            <files include="**/*.resx" buildAction="EmbeddedResource" />
            <files include="**/*.cs" buildAction="Compile" />
        </contentFiles>
    </metadata>
    <files>
        <file src="..\..\bin\GrammarEngine\x64\Release\libgren.so" target="contentFiles\any\any\Resources" />
        <file src="..\..\bin\GrammarEngine\x64\Release\compiler" target="contentFiles\any\any\Resources\compiler" />
        <file src="ResourcesLinux.resx" target="contentFiles\any\any\Properties" />
        <file src="ResourcesLinux.Designer.cs" target="contentFiles\any\any\Properties" />
    </files>
</package>